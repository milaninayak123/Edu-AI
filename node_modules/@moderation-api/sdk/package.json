{
  "name": "@moderation-api/sdk",
  "version": "1.1.3",
  "private": false,
  "description": "Automatically moderate your content with Moderation API",
  "main": "cjs/index.js",
  "types": "types/index.d.ts",
  "author": "Moderation API <info@moderationapi.com> (https://moderationapi.com)",
  "repository": {},
  "engines": {
    "node": ">=16.*"
  },
  "scripts": {
    "generate": "openapi --input 'https://moderationapi.com/api/v1/openapi.json' --output ./src/generated --client axios --name ModerationAPI",
    "build": "yarn build-esm && yarn build-cjs",
    "build-esm": "mkdir -p esm && tsc -p tsconfig.esm.json && echo '{\"type\":\"module\"}' > esm/package.json",
    "build-cjs": "mkdir -p cjs && tsc -p tsconfig.cjs.json && echo '{\"type\":\"commonjs\"}' > cjs/package.json",
    "clean": "rm -rf ./.nyc_output ./node_modules/.cache ./coverage ./esm ./cjs",
    "prepack": "yarn install && yarn build",
    "lint": "eslint --ext .js,.jsx,.ts .",
    "fix": "yarn lint --fix",
    "publish": "npm publish --access public"
  },
  "license": "MIT",
  "homepage": "https://moderationapi.com",
  "keywords": [
    "moderation",
    "detect",
    "personal data",
    "pii",
    "moderate",
    "content moderation",
    "filtering",
    "api",
    "automation",
    "content",
    "toxic",
    "NSFW",
    "analysis",
    "nlp",
    "text analysis",
    "sentiment",
    "spam",
    "image moderation",
    "text moderation"
  ],
  "dependencies": {
    "@types/node": ">=8.1.0",
    "axios": "^1.6.0",
    "form-data": "^4.0.0"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "@typescript-eslint/parser": "^4.33.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-chai-friendly": "^0.7.2",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-prettier": "^3.4.1",
    "openapi-typescript-codegen": "^0.25.0",
    "prettier": "^1.16.4",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.4"
  },
  "exports": {
    "types": "./types/index.d.ts",
    "default": {
      "import": "./esm/index.js",
      "require": "./cjs/index.js"
    }
  }
}
